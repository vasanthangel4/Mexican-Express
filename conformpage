<script type="text/javascript">
$(document).ready(function() {
$('.errortxtp').hide();
$('.errortxtpw').hide();
$('.err_img').hide();
if(window.location.href == 'http://www.chillitalk.co.uk/myaccount/top_up/add_creditcard/#new_card') {
$('#existing_card').hide();
$('#new_card').show();
}else{
$('#new_card').hide();
}
if(window.location.href == 'http://www.chillitalk.co.uk/myaccount/top_up/add_creditcard/#btn_new_card2') {
$('#existing_card').hide();
$('#new_card').show();
}else{
$('#new_card').hide();
}

$('#btn_new_card2').click(function(){
$('#new_card').show();
$('#existing_card').hide();
$('#btn_existing_card').removeAttr('checked','checked');
});
$('#btn_existing_card').click(function(){
$('#new_card').hide();
$('#existing_card').show();
$('#btn_new_card2').removeAttr('checked','checked');
});
$('#text_topup').text('à0');
$('#amount').change(function(){
$('#text_topup').text('à'+$('#amount').val());
if($('#amount').val() =='empty') 
{
$('.errortxtpw').show();
$('.errortxtpw').text('Please choose the credit amount');
}
else {
	$('.errortxtpw').hide();
	}

});
$('#monthc').change(function(){
if($('#monthc').val() =='empty') 
{                   $('.errortxtp').show();
                    $('.errortxtp').text('Please choose the expiry date');
                }
				else if($('#yearc').val() =='empty') 
				{
                    $('.errortxtp').show();
					$('.errortxtp').text('Please choose the expiry date');
                }
				
				else {
				$('.errortxtp').hide();
				}

});

$('#yearc').change(function(){
if($('#monthc').val() =='empty') 
{                   $('.errortxtp').show();
                    $('.errortxtp').text('Please choose the expiry date');
                }
				else if($('#yearc').val() =='empty') 
				{   $('.errortxtp').show(); 
                    $('.errortxtp').text('Please choose the expiry date');
                }
				
				else {
				$('.errortxtp').hide();
				}

});

$('#text_topup2').text('à0');
$('#amount2').change(function(){
$('#text_topup2').text('à'+$('#amount2').val());
})


$('#butonerr2').click(function() {

$('.errortxtp').show();
$('.errortxtpw').show();

if($('#amount').val() =='empty') 
{                   $('.errortxtpw').show();
                    $('.errortxtpw').text('Please choose the credit amount');
                }
				
				else {
				$('.errortxtpw').hide();
				}
				
				if($('#monthc').val() =='empty') 
{
                    $('.errortxtp').text('Please choose the expiry date');
                }
				
				else if($('#yearc').val() =='empty') 
				{
                    $('.errortxtp').text('Please choose the expiry date');
                }
				
				else {
				$('.errortxtp').hide();
				}
				
				
});
});

</script>

<script type="text/javascript">
$(function () {

$('#vasanthhover').mouseover(function () {
$('#vasanthtext').css("display", "block");
});
