<script type='text/javascript'>//<![CDATA[ 
$(function(){
// Tooltip only Text
$('.basetooltip').hover(function () {
  // Hover over code
  var title = $(this).attr('title');
  var container = $('<div id="cvv_info">'

      + '   <b style="padding:4px;">Please enter your CVV(card security code)</b>'
      + '   <p style="text-align:left;padding:4px;margin-top:5px;"><img src="http://www.chillitalk.co.uk/wp-content/themes/chillitalk/img/cvv_info.png" alt="" style="width:110px;float:right;padding-left:5px;"/>'
      + '   The security code is the final three digits of the number printer on the signature strip on the reverse of your card.</p>'


      + '</div>');
  $(this).data('tipText', title).removeAttr('title');
  $('<p class="tooltip"></p>')
    .text(title)
    .appendTo('body')
    .fadeIn('slow')
	.append(container);
	
	
	
}, function () {
  // Hover out code
  $(this).attr('title', $(this).data('tipText'));
  $('.tooltip').remove();
}).mousemove(function (e) {
  var mousex = e.pageX + 10; //Get X coordinates
  var mousey = e.pageY - 110; //Get Y coordinates
  $('.tooltip')
    .css({
    top: mousey,
    left: mousex
  })
});
});//]]>  

</script>
